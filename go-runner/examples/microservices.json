{
  "project_groups": [
    {
      "name": "Backend Services",
      "description": "All backend microservices",
      "color": "#3B82F6"
    },
    {
      "name": "Frontend Apps",
      "description": "Frontend applications",
      "color": "#10B981"
    },
    {
      "name": "Worker Services",
      "description": "Background processing workers",
      "color": "#F59E0B"
    }
  ],
  "microservices": [
    {
      "name": "User Service",
      "description": "User management microservice",
      "type": "backend",
      "group_id": 1,
      "path": "/path/to/user-service",
      "command": "go run main.go",
      "working_dir": "/path/to/user-service",
      "port": 3001,
      "environment": "development",
      "editor": "vscode",
      "health_check_url": "http://localhost:3001/health",
      "auto_restart": true,
      "max_restarts": 3
    },
    {
      "name": "Product Service",
      "description": "Product catalog microservice",
      "type": "backend",
      "group_id": 1,
      "path": "/path/to/product-service",
      "command": "go run main.go",
      "working_dir": "/path/to/product-service",
      "port": 3002,
      "environment": "development",
      "editor": "vscode",
      "health_check_url": "http://localhost:3002/health",
      "auto_restart": true,
      "max_restarts": 3
    },
    {
      "name": "Admin Dashboard",
      "description": "Admin dashboard frontend",
      "type": "frontend",
      "group_id": 2,
      "path": "/path/to/admin-dashboard",
      "command": "npm start",
      "working_dir": "/path/to/admin-dashboard",
      "port": 3000,
      "environment": "development",
      "editor": "vscode",
      "health_check_url": "http://localhost:3000",
      "auto_restart": false
    },
    {
      "name": "Email Worker",
      "description": "Email processing worker",
      "type": "worker",
      "group_id": 3,
      "path": "/path/to/email-worker",
      "command": "python worker.py",
      "working_dir": "/path/to/email-worker",
      "environment": "development",
      "editor": "vscode",
      "auto_restart": true,
      "max_restarts": 5
    },
    {
      "name": "Notification Service",
      "description": "Real-time notification service",
      "type": "backend",
      "group_id": 1,
      "path": "/path/to/notification-service",
      "command": "node server.js",
      "working_dir": "/path/to/notification-service",
      "port": 3003,
      "environment": "development",
      "editor": "vscode",
      "health_check_url": "http://localhost:3003/health",
      "auto_restart": true,
      "max_restarts": 3
    }
  ]
}
